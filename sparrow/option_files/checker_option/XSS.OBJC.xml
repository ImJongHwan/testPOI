            <value>NSMutableString\.substringToIndex:$base->return</value>
            <value>NSString\.dataUsingEncoding:allowLossyConversion:$base->return</value>

            <value>NSMutableURLRequest\.setHTTPBody:$param(1)->base</value>

        </item>
